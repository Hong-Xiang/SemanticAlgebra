{
  "runner": {
    "name": "ubuntu-latest",
    "setup": [
      "sudo apt-get update",
      "sudo apt-get install -y wget apt-transport-https",
      "wget https://dot.net/v1/dotnet-install.sh -O dotnet-install.sh",
      "chmod +x ./dotnet-install.sh",
      "./dotnet-install.sh --channel 9.0",
      "export PATH=\"$HOME/.dotnet:$PATH\"",
      "dotnet --list-sdks"
    ]
  }
}